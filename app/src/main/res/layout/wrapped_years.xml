<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E2FBE3">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/sintony"
        android:text="Wrapped Over The Years"
        android:textColor="#414C41"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton2022"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"

        android:scaleType="fitCenter"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/image_two"
        tools:layout_editor_absoluteX="251dp" />

    <ImageButton
        android:id="@+id/imageButton2021"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/image_three"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="255dp" />

    <ImageButton
        android:id="@+id/imageButton2020"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/image_four"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="255dp" />

    <ImageButton
        android:id="@+id/imageButton2019"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/image_five"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="390dp" />

    <ImageButton
        android:id="@+id/imageButton2018"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/image_six"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="390dp" />

    <ImageButton
        android:id="@+id/imageButton2017"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/image_seven"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="521dp" />

    <ImageButton
        android:id="@+id/imageButton2016"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"
        android:maxHeight="120dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/image_eight"
        tools:layout_editor_absoluteX="251dp"
        tools:layout_editor_absoluteY="521dp" />

    <ImageButton
        android:id="@+id/imageButton2023"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="40dp"
        android:adjustViewBounds="true"
        android:maxWidth="120dp"

        android:maxHeight="120dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageButton2021"
        app:layout_constraintEnd_toStartOf="@+id/imageButton2022"
        app:layout_constraintHorizontal_bias="0.564"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.431"
        app:srcCompat="@drawable/image_one" />

    <Button
        android:id="@+id/settingsbackbutton"
        android:layout_width="384dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#4CAF50"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>